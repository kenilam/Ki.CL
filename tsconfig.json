{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./project",
    "checkJs": false,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "importsNotUsedAsValues": "preserve",
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "es2015",
      "es2015.collection",
      "es2015.iterable",
      "es2017",
      "es2020",
      "scripthost"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "noImplicitAny": false,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "outDir": "./project/build",
    "paths": {
      "@/*": ["src/*"],
      "$/*": ["*"]
    },
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": true,
    "target": "es5"
  },
  "include": ["project/**/*"],
  "sourceMap": false
}
