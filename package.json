{
  "name": "ki-cl",
  "version": "1.0.0",
  "description": "Ki.CL",
  "scripts": {
    "guide": "npx babel-node styleguidist server",
    "test": "NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage",
    "lint": "npm run lint:tslint && npm run lint:stylelint",
    "lint:stylelint": "stylelint 'src/**/*.scss' --fix",
    "lint:tslint": "./node_modules/.bin/tslint '{project,.Webpack}/**/*.{js,jsx,ts,tsx}' --fix",
    "dev": "NODE_ENV=development webpack-dev-server --config .Webpack/development.babel.js",
    "prod": "NODE_ENV=production babel-node .Webpack/production.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "options": [
      "concurrent"
    ],
    "{.Webpack,project}/**/*.{js,jsx,ts,tsx}": [
      "npm run lint:tslint",
      "git add"
    ],
    "project/**/*.scss": [
      "npm run lint:stylelint",
      "git add"
    ]
  },
  "author": "Keni Lam",
  "repository": {
    "type": "git",
    "url": "https://github.com/kenilam/Ki.CL.git"
  },
  "bugs": {
    "url": "https://github.com/kenilam/Ki.CL/issues"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "project/**/*.{js,jsx}",
      "!**/node_modules/**"
    ]
  },
