{
	"presets": [
		"es2015"
	],
	"plugins": [
		"transform-export-extensions",
		[
			"babel-plugin-root-import", [
			{
				"rootPathSuffix": ".",
				"rootPathPrefix": "^"
			},
			{
				"rootPathSuffix": "project/src",
				"rootPathPrefix": "@"
			},
			{
				"rootPathSuffix": "bower_components",
				"rootPathPrefix": "#"
			}
		]
		]
	]
}