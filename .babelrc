{
	"presets" : [
		"es2015",
		"babel-preset-stage-1",
		"react"
	],
	"env": {
		"development": {
			"plugins": [["react-transform", {
				"transforms": [{
					"transform": "react-transform-hmr",
					"imports": ["react"],
					"locals": ["module"]
				}]
			}]]
		}
	},
	"plugins": [
		"transform-export-extensions"
	]
}