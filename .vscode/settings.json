{
  "beautify.tabSize": 2,
  "editor.tabSize": 2,
  "markdown.extension.tableFormatter.normalizeIndentation": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "files.autoGuessEncoding": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "workbench.view.alwaysShowHeaderActions": true,
  "breadcrumbs.enabled": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.settings.useSplitJSON": true,
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "search.usePCRE2": true,
  "debug.inlineValues": true,
  "auto-close-tag.fullMode": true,
  "workbench.fontAliasing": "auto",
  "gotoSymbolStack.currentStackPosition": 0,
  "gotoSymbolStack.maxStackPosition": 0,
  "gotoSymbolStack.filePositionInfo": [],
  "tslint.configFile": "./.tsconfig.json",
  "javascript.implicitProjectConfig.experimentalDecorators": true,
  "javascript.implicitProjectConfig.checkJs": true,
  "god.tsconfig": "./.tsconfig.json",
  "copy-with-imports.path-relative-from-tsconfig": true,
  "workbench.editor.labelFormat": "short",
  "prettier.requireConfig": true,
  "cSpell.language": "en,en-US,en-GB",
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "prettier.stylelintIntegration": true,
  "prettier.proseWrap": "never",
  "prettier.tslintIntegration": true,
  "prettier.jsxBracketSameLine": true,
  "javascript.format.insertSpaceAfterConstructor": true,
  "html.format.endWithNewline": true,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true
}
